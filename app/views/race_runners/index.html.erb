

<h1><%= @race_editions %></h1>


<%= form_for :race_runner, url:{:action => 'create'} do |r| %>
  			<div class="row">
  	
    			<div class="col s5 offset-s1">
		
					<%= r.label :full_name, @race_editions %></br>
					<%= r.text_field :race_edition_id, value: @race_editions %></br> 
				</div>

				<%= r.submit 'Aqui' %>

<% end %>				